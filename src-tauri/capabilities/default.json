{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "store:default",
    "fs:default",
    "dialog:default",
    "fs:write-all",
    "fs:read-all",
    "fs:allow-watch",
    "fs:allow-unwatch",
    "core:window:allow-close",
    "core:window:allow-minimize",
    "core:window:allow-maximize",
    "os:default"
  ]
}